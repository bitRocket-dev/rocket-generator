{
  "name": "rocket-space",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "homepage": "./",
  "description": "bitRocket.dev Space",
  "build": {
    "appId": "com.bitrocketdev.build",
    "extends": null
  },
  "main": "src/@electron/main.js",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@microsoft/signalr": "^5.0.6",
    "aws-amplify": "^3.4.3",
    "connected-react-router": "^6.0.0",
    "conversational-form": "^1.0.2",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "fast-memoize": "^2.5.2",
    "firebase": "^8.3.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "hedron": "^0.7.1",
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.2",
    "react-image": "^4.0.3",
    "react-quick-pinch-zoom": "^4.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-terminal-ui": "^0.1.11",
    "react-transition-group": "^4.4.1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "validate": "create-validator-ts 'src/api/types.ts'",
    "electron": "electron-builder",
    "build:app": "react-scripts build",
    "build:electron": "react-scripts build && npm run electron",
    "build:storybook": "build-storybook -s public",
    "cypress": "./node_modules/.bin/cypress open",
    "generate:component-shared": "node ./@generators/scripts/component-shared/index $1",
    "generate:component-ui": "node ./@generators/scripts/component-ui/index $1",
    "generate:component-view": "node ./@generators/scripts/component-view/index $1",
    "generate:graphql-sdk": "graphql-codegen --config ./codegen.yml",
    "postinstall": "husky install",
    "prettier": "prettier --write src/**/*.{ts,tsx} src/**/**/*.{ts,tsx} src/**/**/**/*.{ts,tsx} src/**/**/**/**/*.{ts,tsx} src/**/**/**/**/**/*.{ts,tsx}",
    "release:electron": "npm run build:app && electron-builder --publish=always",
    "start:build": "npm run build:app && serve -s build",
    "start:dev": "react-scripts start",
    "start:electron": "concurrently \"cross-env BROWSER=none npm run start:dev\" \"wait-on http://localhost:3000 && electron .\"",
    "start:storybook": "start-storybook -p 6006 -s public",
    "test:commit-msg": "commitlint -env HUSKY_GIT_PARAMS",
    "test:eslint": "eslint --ext .ts,.tsx ./src",
    "test:format": "npm run test:lint && npm run test:eslint",
    "test:lint": "tsc --noEmit",
    "upgrade:check": "npm-check",
    "upgrade:interactive": "npm-check --update"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.21.2",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@storybook/addon-actions": "^6.1.19",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.19",
    "@storybook/node-logger": "^6.1.19",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "@types/node": "^12.0.0",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.0.2",
    "create-validator-ts": "^1.1.0",
    "cypress": "^6.5.0",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "eslint": "^7.20.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.0.9",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.9",
    "serve": "^11.3.2",
    "typescript": "^4.1.2",
    "wait-on": "^5.3.0"
  }
}
