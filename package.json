{
  "name": "rocket-generator",
  "version": "1.1.52",
  "description": "Node Generator for bitRocket.dev Community",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "create-rocket-app": "node ./@generators/scripts/create-rocket-app/index $1",
    "generate:component-flow": "node ./@generators/scripts/flow/index $1",
    "generate:component-redux-flow": "node ./@generators/scripts/redux-flow/index $1",
    "generate:component-shared": "node ./@generators/scripts/component-shared/index $1",
    "generate:component-ui": "node ./@generators/scripts/component-ui/index $1",
    "generate:component-view": "node ./@generators/scripts/component-view/index $1",
    "generate:redux-flow": "node ./@generators/scripts/redux-flow/index $1",
    "postcreate-rocket-app": "npm i",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:eslint",
    "prettier": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\"",
    "preversion": "npm run test:lint",
    "start:dev": "npx ts-node src/index.ts",
    "test:commit-msg": "commitlint -env HUSKY_GIT_PARAMS",
    "test:eslint": "eslint --ext .ts,.tsx ./src",
    "test:format": "npm run test:lint",
    "test:lint": "tsc --noEmit",
    "upgrade:check": "npm-check",
    "upgrade:interactive": "npm-check --update",
    "version": "npm run test:format && git add -A src"
  },
  "bin": {
    "rocket-generator": "lib/index.js"
  },
  "keywords": [
    "node-generator",
    "react",
    "redux"
  ],
  "repository": {
    "url": "https://github.com/bitRocket-dev/rocket-generator"
  },
  "author": "bitRocket.dev",
  "license": "ISC",
  "dependencies": {
    "-": "0.0.1",
    "@typescript-eslint/parser": "^4.28.0",
    "boxen": "^5.0.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.29.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  },
  "files": [
    "lib/**/*"
  ]
}
